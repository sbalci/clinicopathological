{
  "name": "histopathR",
  "productName": "clinicopathological",
  "version": "0.0.1",
  "description": "Clinicopathological Research Template",
  "main": "src/index.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint src --color",
    "postinstall": "electron-builder install-app-deps",
    "dev-server": "cross-env NODE_ENV=development webpack-dev-server --config ./webpack.development.js",
    "dev": "concurrently --success first \"npm run dev-server\" \"cross-env NODE_ENV=development electron .\" -k",
    "prod-build": "cross-env NODE_ENV=production npx webpack --mode=production --config ./webpack.production.js",
    "prod": "npm run prod-build && electron .",
    "pack": "electron-builder --dir",
    "dist": "npm run prod-build && electron-builder",
    "dist-mac": "npm run prod-build && electron-builder --mac",
    "dist-linux": "npm run prod-build && electron-builder --linux",
    "dist-windows": "npm run prod-build && electron-builder --windows",
    "dist-all": "npm run prod-build && electron-builder --mac --linux --windows"
  },
  "keywords": [
    "R",
    "R-project",
    "pathology",
    "histopathology",
    "clinicopathological",
    "ClinicoPath",
    "shiny",
    "research",
    "histopathR",
    "biostatistics",
    "electron",
    "security",
    "template",
    "react",
    "redux",
    "webpack",
    "i18next",
    "boilerplate"
  ],
  "author": "Serdar Balci, MD, Pathologist",
  "license": "MIT",
  "private": true,
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "config": {
    "forge": {
      "make_targets": {
        "win32": [
          "squirrel"
        ],
        "darwin": [
          "zip"
        ]
      },
      "electronPackagerConfig": {
        "packageManager": "npm",
        "afterCopy": [
          "./prune-r"
        ]
      },
      "electronWinstallerConfig": {
        "name": "clinicopathological"
      },
      "electronInstallerDebian": {},
      "electronInstallerRedhat": {},
      "github_repository": {
        "owner": "sbalci",
        "name": "clinicopathological"
      },
      "windowsStoreConfig": {
        "packageName": "clinicopathological",
        "name": "clinicopathological"
      }
    }
  },
  "dependencies": {
    "@hapi/cryptiles": "^5.1.0",
    "@hapi/hoek": "^9.0.4",
    "@reduxjs/toolkit": "^1.4.0",
    "acorn": "^7.3.1",
    "axios": "^0.21.1",
    "btoa": "^1.2.1",
    "clean-css": "^4.2.3",
    "connected-react-router": "^6.8.0",
    "constantinople": "^4.0.1",
    "debug": "^4.1.1",
    "easy-redux-undo": "^1.0.1",
    "electron-compile": "^6.4.4",
    "electron-squirrel-startup": "^1.0.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "execa": "^3.4.0",
    "i18next": "^19.5.4",
    "i18next-electron-fs-backend": "^1.3.2",
    "immutable": "^3.8.2",
    "js-yaml": "^3.14.0",
    "lodash": "4.17.19",
    "lodash.merge": "^4.6.2",
    "mem": "^6.1.0",
    "mime": "^2.4.6",
    "node-gyp": "^7.0.0",
    "q": "^1.5.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-i18next": "^11.5.1",
    "react-redux": "^7.2.0",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "rebuild": "^0.1.2",
    "redux": "^4.0.5",
    "seamless-immutable": "^7.1.4",
    "secure-electron-context-menu": "^1.2.1",
    "secure-electron-store": "^1.2.0",
    "semver": "^7.3.2",
    "uglify-js": "^3.10.0"
  },
  "devDependencies": {
    "@babel/core": "^7.10.4",
    "@babel/plugin-transform-react-jsx": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "babel-loader": "^8.1.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "concurrently": "^5.2.0",
    "cross-env": "^7.0.2",
    "csp-html-webpack-plugin": "^4.0.0",
    "css-loader": "^3.6.0",
    "electron": "^8.3.4",
    "electron-builder": "^22.7.0",
    "electron-debug": "^3.1.0",
    "electron-devtools-installer": "^3.1.0",
    "electron-forge": "^5.2.4",
    "electron-prebuilt-compile": "4.0.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.3",
    "fs-extra": "^8.1.0",
    "html-loader": "^1.1.0",
    "html-webpack-plugin": "^4.3.0",
    "mini-css-extract-plugin": "^0.9.0",
    "style-loader": "^1.2.1",
    "url-loader": "^4.1.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^4.2.2"
  }
}
